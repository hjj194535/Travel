import '~styles/varibles.styl'
ellipsis()
  overflow: hidden
  white-space: nowarp
  text-overflow: ellipsis

indexTitle()
  border-sizing: border-box
  overflow: hidden
  width: 100%
  height: 0
  padding:.32rem 0 .26rem 0
  padding-bottom: 6%

indexTitleContent()
  display: inline-block
  margin-left: .08rem
  height: .44rem
  font-size: .32rem
  line-height: .44rem
  color: $darkTextColor

indexTitleImg()
  display: inline-block
  overflow: hidden
  width: .3rem
  height: .3rem
  margin-left: .26rem
  vertical-align: top				
					
					
					
					
